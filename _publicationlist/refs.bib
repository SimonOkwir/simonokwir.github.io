  author = {Gustafsson, Stefan and Gedon, Daniel and Lampa, Erik and Ribeiro, Ant{\^o}nio H. and Holzmann, Martin J. and Sch{\"o}n, Thomas B. and Sundstr{\"o}m, Johan},
  year = {2022},
