@article{abdalmoaty_application_2018,
  title = {Application of a {{Linear PEM Estimator}} to a {{Stochastic Wiener-Hammerstein Benchmark Problem}}},
  author = {Abdalmoaty, Mohamed Rasheed and Hjalmarsson, H{\aa}kan},
  year = {2018},
