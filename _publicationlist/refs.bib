  pmid = {34565171},
  keywords = {Chagas cardiomyopathy,Chagas Cardiomyopathy,Chagas disease,disease progression,Disease Progression,Female,Humans,Incidence,Male,Middle Aged,mortality,serology,Trypanosoma cruzi},
