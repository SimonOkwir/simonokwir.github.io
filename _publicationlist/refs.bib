@article{calafiore_sparse_2015,
  title = {Sparse Identification of Posynomial Models},
  author = {Calafiore, Giuseppe C. and El Ghaoui, Laurent M. and Novara, Carlo},
  year = {2015},
  month = sep,
  journal = {Automatica},
